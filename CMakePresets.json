{
  "version": 3,
  "cmakeMinimumRequired": {
    "major": 3,
    "minor": 22,
    "patch": 0
  },
  "configurePresets": [
    {
      "name": "debug",
      "displayName": "Debug Config",
      "description": "debug ebug build using Ninja generator",
      "generator": "Ninja",
      "environment": {
      },
      "cacheVariables": {
        "CMAKE_BUILD_TYPE": "Debug",
        "ENABLE_LINUX_INSTRUMENTATION": "ON",
        "CMAKE_INSTALL_PREFIX": "/tmp/phongdefo",
        "CMAKE_MODULE_PATH": "$env{CMAKE_UTIL_PATH};$env{CMAKE_MODULE_PATH}"
      }
    },
    {
      "name": "ubuntu-debug",
      "displayName": "Ubuntu Debug Config",
      "description": "Ubuntu debug ebug build using Ninja generator",
      "inherits": "debug",
      "generator": "Ninja",
      "binaryDir": "${sourceDir}/build/ubuntu-debug",
      "environment": {
      },
		"cacheVariables": {
			"nanoflann_DIR" : "$env{HOME}/nanoflann/1.7.0/share/cmake/nanoflann"
      }
    },
    {
      "name": "ubuntu-h195-debug",
